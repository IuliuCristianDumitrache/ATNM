<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DividerViewStyle">
        <item name="android:layout_height">@dimen/extra_small</item>
        <item name="android:background">@color/palette_pure_black</item>
    </style>

    <style name="DescriptionLightTextStyle">
        <item name="android:textSize">@dimen/text_regular</item>
        <item name="android:textStyle">normal</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/palette_pure_white</item>
    </style>

    <style name="SmallDescriptionTextStyle">
        <item name="android:textSize">@dimen/text_bigger_regular</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/palette_pure_white</item>
    </style>
</resources>